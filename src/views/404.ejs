<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>404</title>
</head>

<body>
 

    <span id="server-msg">
    </span>
</body>


<script>
    const serverMessage = document.getElementById("server-msg");

    fetch("http://localhost:5000/404")
        .then((response) => {
            const data = response.json();
            serverMessage.innerText += `${data.message}`;
            console.log(data)
        }).catch((err) => {
            serverMessage.innerText += `${err}`;
        });
</script>

</html>